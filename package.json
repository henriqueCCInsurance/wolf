{
  "name": "wolf-den",
  "version": "1.2.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "deploy": "./deploy.sh",
    "deploy:manual": "npm run build && echo 'Build complete! Now drag the dist/ folder to https://app.netlify.com'",
    "netlify": "npx netlify-cli@latest",
    "netlify:dev": "npx netlify-cli@latest dev",
    "netlify:deploy": "npx netlify-cli@latest deploy",
    "netlify:deploy:prod": "npx netlify-cli@latest deploy --prod",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "netlify dev:exec drizzle-kit migrate",
    "db:studio": "netlify dev:exec drizzle-kit studio"
  },
  "dependencies": {
    "@netlify/neon": "^0.1.0",
    "@supabase/supabase-js": "^2.50.3",
    "@types/leaflet": "^1.9.19",
    "@types/papaparse": "^5.3.16",
    "@types/react-router-dom": "^5.3.3",
    "@types/three": "^0.160.0",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "date-fns": "^3.3.1",
    "isomorphic-dompurify": "^2.9.0",
    "drizzle-orm": "^0.44.2",
    "framer-motion": "^10.18.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.344.0",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1",
    "three": "^0.160.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.4.0"
  }
}
